{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "func",
      "label": "func: host start",
      "command": "host start",
      "dependsOn": [
        "build"
      ],
      "problemMatcher": "$func-watch",
      "isBackground": true
    },
    {
      "type": "cargo",
      "label": "cargo: build",
      "command": "build",
      "problemMatcher": [
        "$rustc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
